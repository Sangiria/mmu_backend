{
  "kind": "collectionType",
  "collectionName": "groups",
  "info": {
    "singularName": "group",
    "pluralName": "groups",
    "displayName": "Group"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "group_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "degree_program": {
      "type": "string",
      "required": true
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student",
      "mappedBy": "group"
    },
    "applications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::application.application",
      "mappedBy": "group"
    }
  }
}
